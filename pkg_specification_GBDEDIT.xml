<Label Align="Right" overlap="pre" Size="120,20" Name="lb_specification_id" Visible="false" Text="specification_id" readonly="true"/>
<TextBox overlap="pre" Weight="110" Size="110,20" Name="specification_id" Visible="false" Text="" ToolTip="ttp_specification_id" readonly="true"><OnEnChange><if cond="'%{specification_id}'"><SetVisible name="insertpkg_specification" Visible="0"/><SetVisible name="modifypkg_specification" Visible="1"/><else/><SetVisible name="insertpkg_specification" Visible="1"/><SetVisible name="modifypkg_specification" Visible="0"/></if></OnEnChange></TextBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>